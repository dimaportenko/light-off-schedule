{
  "name": "light-off-schedule",
  "version": "1.0.0",
  "scripts": {
    "start:normal": "npx expo start",
    "start:dev": "npx expo start --dev-client",
    "start": "npx expo start",
    "android": "npx expo run:android",
    "android:device": "npx expo run:android --device",
    "android:release:device": "npx expo run:android --variant Release --device",
    "eas:preview": "eas build --profile preview",
    "eas:preview:all": "eas build --profile preview --platform all",
    "eas:preview:ios:local": "eas build --profile preview --platform ios --local",
    "eas:preview:android:local": "eas build --profile preview --platform android --local",
    "eas:development:local": "eas build --profile development --local",
    "eas:development:all": "eas build --profile development --platform all",
    "eas:update": "eas update --auto",
    "generate:db": "npx ts-node ./scripts/generate-db.ts",
    "ios": "npx expo run:ios",
    "ios:release:device": "npx expo run:ios --configuration Release -d",
    "ios:debug:device": "npx expo run:ios --configuration Debug -d",
    "validate": "npx tsc",
    "test": "TZ=Europe/Kiev jest __tests__",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.5.3",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@types/jest": "^29.2.5",
    "dayjs": "^1.11.7",
    "expo": "~47.0.12",
    "expo-dev-client": "~2.0.1",
    "expo-localization": "~14.0.0",
    "expo-notifications": "~0.17.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-updates": "~0.15.6",
    "i18n-js": "3.9.2",
    "mobx": "^6.7.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^3.4.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-modal-datetime-picker": "^14.0.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.9",
    "twrnc": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.1",
    "@testing-library/react-native": "^11.5.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/i18n-js": "3.8.2",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.70.6",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-testing-library": "^5.10.0",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.1",
    "prettier": "^2.8.1",
    "timekeeper": "^2.2.0",
    "typescript": "^4.6.3"
  },
  "private": true,
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}
