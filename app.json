{
  "expo": {
    "name": "Розклад Відключень",
    "slug": "light-off-schedule",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plabpreview.light-off-schedule",
      "buildNumber": "10"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "googleServicesFile": "./assets/android/google-services.json",
      "package": "com.plabpreview.lightoffschedule",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM"
      ],
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7027993e-c322-41ae-8794-d20e4296b806"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/android/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
